{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-mixed-spaces-and-tabs": "warn",
        "no-unused-vars": "warn"
    },
    "globals": {
        "__dirname": "readonly",
        "process": "readonly"
    }
}
